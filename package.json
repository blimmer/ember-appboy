{
  "name": "ember-appboy",
  "version": "0.2.2",
  "description": "Integrates the appboy web sdk into your ember app.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember try:each",
    "deploy": "ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\"; git push; git checkout -"
  },
  "repository": "https://github.com/blimmer/ember-appboy",
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "Ben Limmer <hello@benlimmer.com> (http://benlimmer.com/)",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.2",
    "ember-ajax": "^2.0.1",
    "ember-cli": "2.8.0",
    "ember-cli-app-version": "^1.0.0",
    "ember-cli-dependency-checker": "^1.2.0",
    "ember-cli-github-pages": "0.1.2",
    "ember-cli-htmlbars": "^1.0.3",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-inject-live-reload": "^1.4.0",
    "ember-cli-qunit": "3.0.1",
    "ember-cli-release": "1.0.0-beta.2",
    "ember-cli-sass": "5.5.1",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-test-loader": "^1.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-code-snippet": "1.4.0",
    "ember-data": "^2.8.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^1.0.5",
    "ember-font-awesome": "2.2.0",
    "ember-load-initializers": "^0.5.1",
    "ember-markdown-section": "0.0.5",
    "ember-paper": "1.0.0-alpha.3",
    "ember-resolver": "^2.0.3",
    "ember-sinon": "0.5.1",
    "ember-sinon-qunit": "1.3.4",
    "loader.js": "^4.0.1"
  },
  "keywords": [
    "ember-addon",
    "appboy",
    "appboy-web-sdk"
  ],
  "dependencies": {
    "appboy-web-sdk": "^1.6.1",
    "broccoli-funnel": "^1.0.6",
    "broccoli-merge-trees": "^1.1.4",
    "ember-cli-babel": "^5.1.6",
    "ouibounce": "0.0.12"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://blimmer.github.io/ember-appboy",
    "versionCompatibility": {
      "ember": ">= 2.3"
    }
  }
}
